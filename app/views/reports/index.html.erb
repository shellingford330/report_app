<% provide(:title, "指導報告書")%>
<div class="container mt-3">
  <section id="posts">
    <div class="row">
      <% @reports.each do |report| %>
        <div class="col-md-4">
          <%= render 'reports/report', report: report %>
        </div>
      <% end %>
      <div class="col-md-12">
        <%= will_paginate @reports, :previous_label => '&laquo', :next_label => '&raquo' %>
      </div>
    </div>
  </section>
</div>